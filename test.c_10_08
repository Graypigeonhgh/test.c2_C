//把输入的3个数按顺序排列
int main()
{
	int a=0;
	int b=0;
	int c=0;
	scanf("%d,%d,%d",&a,&b,&c);//必须按格式输入1,2,3
	if(a<b)
	{
		int tmp=a;
		a=b;
		b=tmp;
	}
	if(a<c)
	{
		int tmp=a;
		a=b;
		b=tmp;
	}
	if(b<c)
	{
		int tmp=b;
		b=c;
		c=tmp;
	}
	printf("%d %d %d\n",a,b,c);
	return 0;
}
